<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- Vanilla Insectoids TODO: LavaSnail-->

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="AlphaThrumbo"]</xpath>
		<value>
			<li Class="Xenomorphtype.AnimalHostGenes">
				<genes>
					<li MayRequire="Ludeon.RimWorld.Biotech">Skin_SheerWhite</li>
					<li MayRequire="Ludeon.RimWorld.Biotech">Robust</li>
					<li MayRequire="Ludeon.RimWorld.Biotech">Pain_Reduced</li>
					<li MayRequire="Ludeon.RimWorld.Biotech">MeleeDamage_Strong</li>
					<li>XMT_Thrumbo_Features</li>
					<li MayRequire="Ludeon.RimWorld.Biotech">Body_Hulk</li>
					<li MayRequire="RedMattis.BigSmall.Core">HalfJotunFrame</li>
				</genes>
			</li>
			<li Class="Xenomorphtype.AnimalMutateForms">
				<resultCandidates>
					<li>
						<thingTransformationTarget>XMT_AbominableMaw</thingTransformationTarget>
						<pawnTransformationTarget>XMT_AbominableMaw</pawnTransformationTarget>
						<probability>1</probability>
						<essenceMinimum>-1</essenceMinimum>
						<essenceMaximum>-1</essenceMaximum>
					</li>
				</resultCandidates>
				<horrorPregnancy>XMT_ThrumboHorrorPregnant</horrorPregnancy>
			</li>
		</value>
	</Operation>

