<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- For People Modding custom Genes for a HAR race you need to xpath to Defs/AlienRace.ThingDef_AlienRace instead of ThingDef-->
<!-- Vanilla Insectoids TODO: LavaSnail-->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Megascarab"]</xpath>
		<value>
			<li Class="Xenomorphtype.AnimalHostGenes">
				<genes>
					<li MayRequire="vanillaracesexpanded.insector">VRE_Hiveglands</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_HardenedChitin</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_HypothermicHibernation</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_MegascarabInsectoidSkin_Cosmetic</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_InsectPheromones</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_GenelineEvolution</li>
					<li MayRequire="vanillaracesexpanded.insector">VRE_InsectMandibles</li>
					<li MayRequire="Ludeon.RimWorld.Biotech">Body_Thin</li>
				</genes>
			</li>
			<li Class="Xenomorphtype.AnimalMutateForms">
				<resultCandidates>
					<li>
						<thingTransformationTarget>XMT_Hammerpede</thingTransformationTarget>
						<pawnTransformationTarget>XMT_Hammerpede</pawnTransformationTarget>
						<probability>1</probability>
						<essenceMinimum>-1</essenceMinimum>
						<essenceMaximum>-1</essenceMaximum>
					</li>
				</resultCandidates>
				<horrorPregnancy>XMT_InsectHorrorPregnant</horrorPregnancy>
			</li>
		</value>
	</Operation>
	
