<Defs>
	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_Protofluid</defName>
		<label>discoloration</label>
		<description>A discoloration spreads out from an injection site.</description>
		<makesSickThought>true</makesSickThought>
		<hediffClass>Hediff_Injury</hediffClass>
		<makesAlert>true</makesAlert>
		<minSeverity>0.001</minSeverity>
		<maxSeverity>1</maxSeverity>
		<initialSeverity>1</initialSeverity>
		<stages>
			<li>
				<label>faded</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.4</minSeverity>
				<hungerRateFactorOffset>0.1</hungerRateFactorOffset>
			</li>
			<li>
				<label>dissipating</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.7</minSeverity>
				<hungerRateFactorOffset>0.2</hungerRateFactorOffset>
			</li>
			<li>
				<label>metabolizing</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.8</minSeverity>
				<hungerRateFactorOffset>1</hungerRateFactorOffset>
			</li>
			<li>
				<label>spreading</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.9</minSeverity>
				<hungerRateFactorOffset>0.2</hungerRateFactorOffset>
			</li>
			<li>
				<label>initial</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>1.0</minSeverity>
				<hungerRateFactorOffset>0.1</hungerRateFactorOffset>
			</li>
		</stages>
		<comps>
		    <li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-1</severityPerDay>
			</li>
			<li Class="Xenomorphtype.HediffCompProperties_Mutator">
				<probability>0.75</probability>
				<mutateHourInterval>0.25</mutateHourInterval>
				<customMutationSet>XMT_ProtofluidMutationSet</customMutationSet>
			</li>
		</comps>
		<injuryProps>
		  <painPerSeverity>0.1</painPerSeverity>
		  <bleedRate>0.0</bleedRate>
		  <canMerge>true</canMerge>
		  <destroyedLabel>Dissolved</destroyedLabel>
		</injuryProps>
	</HediffDef>

