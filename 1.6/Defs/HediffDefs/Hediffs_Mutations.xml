	<Xenomorphtype.XMT_TransformationSet>
		<defName>XMT_DefaultTransformationSet</defName>
		<transformations>
			<li>
				<thingTransformationTarget></thingTransformationTarget>
				<pawnTransformationTarget>XMT_StarbeastKind</pawnTransformationTarget>
				<probability>1</probability>
				<essenceMinimum>1</essenceMinimum>
				<essenceMaximum>2</essenceMaximum>
			</li>
		</transformations>
	</Xenomorphtype.XMT_TransformationSet>
	
	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_Protofluid</defName>
		<label>discoloration</label>
		<description>A discoloration laces the tissue.</description>
		<makesSickThought>false</makesSickThought>
		<makesAlert>false</makesAlert>
		<minSeverity>0.000</minSeverity>
		<maxSeverity>1</maxSeverity>
		<initialSeverity>1</initialSeverity>
		<stages>
			<li>
				<label>benign</label>
				<becomeVisible>false</becomeVisible>
				<minSeverity>0.1</minSeverity>
				<partEfficiencyOffset>-0.05</partEfficiencyOffset>
				<hungerRateFactorOffset>0.1</hungerRateFactorOffset>
			</li>
		</stages>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_Obsession">
				<obsessionGain>0.125</obsessionGain>
			</li>
			<li Class="Xenomorphtype.HediffCompProperties_Mutator">
				<probability>0.5</probability>
				<customMutationSet>XMT_ProtofluidMutationSet</customMutationSet>
			</li>
		</comps>
	</HediffDef>
	
	
	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_MildEssence</defName>
		<label>foreign tissue</label>
		<description>A foreign growth of discoloration laces the tissue.</description>
		<makesSickThought>false</makesSickThought>
		<makesAlert>true</makesAlert>
		<minSeverity>0.000</minSeverity>
		<maxSeverity>1</maxSeverity>
		<hediffClass>Hediff_Injury</hediffClass>
		<displayWound>true</displayWound>
		<initialSeverity>0.01</initialSeverity>
		<stages>
			<li>
				<label>benign</label>
				<becomeVisible>false</becomeVisible>
				<partEfficiencyOffset>-0.05</partEfficiencyOffset>
				<hungerRateFactorOffset>1</hungerRateFactorOffset>
			</li>
			<li>
				<label>benign</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.1</minSeverity>
				<partEfficiencyOffset>-0.05</partEfficiencyOffset>
				<hungerRateFactorOffset>0.1</hungerRateFactorOffset>
			</li>
		</stages>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_Obsession">
				<obsessionGain>0.125</obsessionGain>
			</li>
		</comps>
		<injuryProps>
		  <painPerSeverity>0.0</painPerSeverity>
		  <bleedRate>0.0</bleedRate>
		  <canMerge>true</canMerge>
		  <destroyedLabel>Dissolved</destroyedLabel>
		</injuryProps>
	</HediffDef>
	
	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_BoostingEssence</defName>
		<label>foreign tissue</label>
		<description>A foreign growth of discoloration laces the tissue.</description>
		<makesSickThought>false</makesSickThought>
		<makesAlert>true</makesAlert>
		<minSeverity>0.000</minSeverity>
		<maxSeverity>1</maxSeverity>
		<initialSeverity>0.01</initialSeverity>
		<hediffClass>Hediff_Injury</hediffClass>
		<displayWound>true</displayWound>
		<stages>
			<li>
				<label>benign</label>
				<becomeVisible>true</becomeVisible>
				<partEfficiencyOffset>0.15</partEfficiencyOffset>
				<hungerRateFactorOffset>0.0</hungerRateFactorOffset>
			</li>
			<li>
				<label>beneficial</label>
				<becomeVisible>true</becomeVisible>
				<minSeverity>0.1</minSeverity>
				<partEfficiencyOffset>0.15</partEfficiencyOffset>
				<hungerRateFactorOffset>0.1</hungerRateFactorOffset>
			</li>
		</stages>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_Obsession">
				<obsessionGain>0.001</obsessionGain>
			</li>
			<li Class="Xenomorphtype.HediffCompProperties_PerPartPromoter">
				<HediffPromotionsByPart>
					<li>
						<hediffDef>XMT_ImprovedStomach</hediffDef>
						<bodyPartDef>Stomach</bodyPartDef>
					</li>
				</HediffPromotionsByPart>
			</li>
		</comps>
		<injuryProps>
		  <painPerSeverity>0.0</painPerSeverity>
		  <bleedRate>0.0</bleedRate>
		  <canMerge>true</canMerge>
		  <destroyedLabel>Dissolved</destroyedLabel>
		</injuryProps>
	</HediffDef>
	
	<HediffDef Name="AddedXenomorphPart" ParentName="AddedBodyPartBase" Abstract="True">
		<organicAddedBodypart>true</organicAddedBodypart>
	</HediffDef>
	
	<HediffDef ParentName="AddedXenomorphPart">
		<defName>XMT_ImprovedStomach</defName>
		<label>carbo-silicate stomach</label>
		<labelNoun>a carbo-silicate stomach</labelNoun>
		<description>a complex new polymer has replaced the lining of the stomach. the acid within has developed a significantly greater strength while mucus membranes protect the rest of the tissues.</description>
		<defaultInstallPart>Stomach</defaultInstallPart>
		<stages>
			<li>
				<foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
				<makeImmuneTo>
					<li>GutWorms</li>
				</makeImmuneTo>
			</li>
		</stages>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_Obsession">
				<obsessionGain>0.125</obsessionGain>
			</li>
			<li Class="Xenomorphtype.HediffCompProperties_Mutator">
				<probability>0.01</probability>
			</li>
		</comps>
	</HediffDef>
	
	<HediffDef ParentName="XenomorphtypeBase">
		<defName>XMT_BecomingMutant</defName>
		<label>transformative cell division</label>
		<description>cells are rapidly dividing and self destructiong at an alarming rate.</description>
		<makesSickThought>false</makesSickThought>
		<makesAlert>true</makesAlert>
		<minSeverity>0.0</minSeverity>
		<maxSeverity>1.0</maxSeverity>
		<hediffClass>HediffWithComps</hediffClass>
		<stages>
			<li>
				
			</li>
		</stages>
		<comps>
			<li Class="Xenomorphtype.HediffCompProperties_Obsession">
				<obsessionGain>0.25</obsessionGain>
			</li>
			<li Class="Xenomorphtype.HediffCompProperties_Transformation">
				<DeathTriggerSeverity>0.5</DeathTriggerSeverity>
				<SeverityPerDay>1</SeverityPerDay>
			</li>
		</comps>
	</HediffDef>
