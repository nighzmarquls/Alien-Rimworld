	<Xenomorphtype.XMT_MutationsHealthSet>
		<defName>XMT_MutationsSet</defName>
		<mutations>
			<li>
				<horror>XMT_RawJellyHigh</horror>
				<randomBodypart>false</randomBodypart>
				<probability>0.85</probability>
				<essenceMinimum>0</essenceMinimum>
				<essenceMaximum>0.1</essenceMaximum>
			</li>
			<li>
				<horror>XMT_MildEssence</horror>
				<randomBodypart>true</randomBodypart>
				<probability>0.75</probability>
				<essenceMinimum>0</essenceMinimum>
				<essenceMaximum>0.45</essenceMaximum>
			</li>
			<li>
				<horror>XMT_BoostingEssence</horror>
				<randomBodypart>true</randomBodypart>
				<probability>0.25</probability>
				<essenceMinimum>0.5</essenceMinimum>
				<essenceMaximum>0.75</essenceMaximum>
			</li>
			<li>
				<horror>XMT_ImprovedStomach</horror>
				<randomBodypart>false</randomBodypart>
				<probability>0.25</probability>
				<essenceMinimum>0.5</essenceMinimum>
				<essenceMaximum>2</essenceMaximum>
			</li>
			<li>
				<horror>XMT_EssenceAddiction</horror>
				<randomBodypart>false</randomBodypart>
				<probability>0.25</probability>
				<essenceMinimum>0</essenceMinimum>
				<essenceMaximum>0.8</essenceMaximum>
			</li>
			<li>
				<horror>XMT_Lardering</horror>
				<randomBodypart>false</randomBodypart>
				<probability>0.1</probability>
				<essenceMinimum>2</essenceMinimum>
				<essenceMaximum>3</essenceMaximum>
			</li>
			<li>
				<horror>XMT_BecomingMutant</horror>
				<randomBodypart>false</randomBodypart>
				<probability>1</probability>
				<essenceMinimum>3</essenceMinimum>
				<essenceMaximum>10</essenceMaximum>
			</li>
		</mutations>
	</Xenomorphtype.XMT_MutationsHealthSet>
	
