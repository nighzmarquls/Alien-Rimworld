	<GeneDef ParentName="GeneSkinColorOverride">
		<defName>XMT_Muffalo_Color</defName>
		<geneClass>Xenomorphtype.XenomorphGene</geneClass>
		<label>pale blue skin</label>
		<labelShortAdj>pale blue</labelShortAdj>
		<description>Carriers of this gene produce a pigment that turns their skin and hair a pale blue color.</description>
		<selectionWeight>0</selectionWeight>
		<skinColorOverride>(176, 195, 201)</skinColorOverride>
		<hairColorOverride>(176, 195, 201)</hairColorOverride>
		<displayOrderInCategory>88</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>blue</symbol></li>
				<li><symbol>ice</symbol></li>
				<li><symbol>sky</symbol></li>
			</prefixSymbols>
