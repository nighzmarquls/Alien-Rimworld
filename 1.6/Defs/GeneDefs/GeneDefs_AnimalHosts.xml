<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<GeneDef>
		<defName>XMT_Chemfuel_Metabolism</defName>
		<geneClass>Xenomorphtype.ChemfuelGene</geneClass>
		<label>chemfuel saturating metabolism</label>
		<labelShortAdj>booma</labelShortAdj>
		<iconPath>UI/GeneIcons/Gene_Boomalope</iconPath>
		<description>Carriers of this gene have internal organs and enzymes which saturate their tissues with chemfuel.</description>
		<displayOrderInCategory>68</displayOrderInCategory>
		<selectionWeight>0</selectionWeight>
		<biostatCpx>1</biostatCpx>
		<biostatMet>5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>booma</symbol></li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">		
				<hideGene>true</hideGene>
			</li>
		</modExtensions>
	</GeneDef>
  
	<GeneDef>
	<defName>XMT_Thrumbo_Features</defName>
