		<displayOrderInCategory>69</displayOrderInCategory>
		<selectionWeight>0</selectionWeight>
		<biostatCpx>1</biostatCpx>
		<biostatMet>5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>thrum</symbol></li>
			</prefixSymbols>
		</symbolPack>
		<exclusionTags>
			<li MayRequire="ludeon.rimworld.biotech">Headbone</li>
