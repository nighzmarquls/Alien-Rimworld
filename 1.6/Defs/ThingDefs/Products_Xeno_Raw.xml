<Defs>
	<ThingDef>
		<defName>Starbeast_Flesh_Meat</defName>
		<label>carbo-silicate meat.</label>
		<description>A strangely tough meat-like material. Full of fibrous polymers and sharp silicate crystals. It tastes like burnt plastic and cuts the mouth if eaten.</description>
		<thingClass>ThingWithComps</thingClass>
		<selectable>true</selectable>
		<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<altitudeLayer>Item</altitudeLayer>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<drawGUIOverlay>true</drawGUIOverlay>
		<healthAffectsPrice>false</healthAffectsPrice>
		<stackLimit>75</stackLimit>
		<alwaysHaulable>true</alwaysHaulable>
		<socialPropernessMatters>true</socialPropernessMatters>
		<tradeability>Sellable</tradeability>
		<pathCost>14</pathCost>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<Beauty>-6</Beauty>
			<DeteriorationRate>0</DeteriorationRate>
			<Mass>0.03</Mass>
			<Flammability>0</Flammability>
			<Nutrition>0.025</Nutrition>
			<FoodPoisonChanceFixedHuman>1</FoodPoisonChanceFixedHuman>
			<MarketValue>0.5</MarketValue>
		</statBases>
		<graphicData>
			<texPath>Things/Item/Resource/Starbeast_Flesh</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<thingCategories>
			<li>MeatRaw</li>
		</thingCategories>
		<ingredient>
			<mergeCompatibilityTags>
				<li  MayRequire="Ludeon.RimWorld.Anomaly">TwistedMeat</li>
			</mergeCompatibilityTags>
		</ingredient>
		<ingestible>
			<foodType>Meat</foodType>
			<preferability>RawBad</preferability>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<specialThoughtDirect MayRequire="Ludeon.RimWorld.Anomaly">AteTwistedMeat</specialThoughtDirect>
			<specialThoughtAsIngredient MayRequire="Ludeon.RimWorld.Anomaly">AteTwistedMeat</specialThoughtAsIngredient>
		</ingestible>
		<comps>
			<li Class="CompProperties_Forbiddable" />
		</comps>
	</ThingDef>
	
	<ThingDef ParentName="OrganicProductBase">
		<defName>Starbeast_Jelly</defName>
		<thingClass>Medicine</thingClass>
		<label>resinous jelly</label>
		<description>A strange resinous jelly. It is simultaneously readily digestible, milky-sweet, anti-microbial, anti-viral and wound clotting. It does not rot.</description>
		<possessionCount>10</possessionCount>
		<graphicData>
			<texPath>Things/Item/Resource/Starbeast_Jelly</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>8.0</MarketValue>
			<Mass>0.025</Mass>
			<Nutrition>0.05</Nutrition>
			<FoodPoisonChanceFixedHuman>0.0</FoodPoisonChanceFixedHuman>
			<MedicalPotency>0.60</MedicalPotency>
			<MedicalQualityMax>0.70</MedicalQualityMax>
			<DeteriorationRate>0</DeteriorationRate>
			<WorkToMake>300</WorkToMake>
		</statBases>
		<costList>
			<Starbeast_Flesh_Meat>10</Starbeast_Flesh_Meat>
		</costList>
		<ingestible>
			<foodType>AnimalProduct</foodType>
			<preferability>MealFine</preferability>
			<joy>0.08</joy>
			<joyKind>Gluttonous</joyKind>
			<ingestEffect>ResinBuild</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<lowPriorityCaravanFood>true</lowPriorityCaravanFood>
			<babiesCanIngest>true</babiesCanIngest>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>XMT_RawJellyHigh</hediffDef>
					<severity>1.00</severity>
					<doToGeneratedPawnIfAddicted>true</doToGeneratedPawnIfAddicted>
				</li>
				<li Class="IngestionOutcomeDoer_OffsetPsyfocus">
					<offset>0.001</offset>
				</li>
			</outcomeDoers>
		</ingestible>
		<recipeMaker>
			<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>DrugLab</li>
				<li>ElectricStove</li>
				<li>FueledStove</li>
				<li>Brewery</li>
			</recipeUsers>
			<researchPrerequisites>
				<li>XMT_Jelly_Extraction</li>
			</researchPrerequisites>
			<skillRequirements>
				<Crafting>4</Crafting>
				<Intellectual>4</Intellectual>
				<Medicine>10</Medicine>
			</skillRequirements>
			<bulkRecipeCount>10</bulkRecipeCount>
			<displayPriority>2000</displayPriority>
		</recipeMaker>
		<thingCategories>
			<li>AnimalProductRaw</li>
			<li>Medicine</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_Drug">
				<chemical>XMT_Essence</chemical>
				<addictiveness>0.01</addictiveness>
				<needLevelOffset>0.9</needLevelOffset>
				<listOrder>1010</listOrder>
			</li>
		</comps>
		<allowedArchonexusCount>150</allowedArchonexusCount>
		<modExtensions>
			<li MayRequire="Dubwise.DubsBadHygiene" Class="DubsBadHygiene.WaterExt">
				<water>1</water>
				<UseForWashing>false</UseForWashing>
				<SeekForThirst>true</SeekForThirst>
			</li>
		</modExtensions>
	</ThingDef>
	
